{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "cms-messages-subscription": "job-profile-metadata",
    "cms-messages-topic": "cms-messages-localdev",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet",
    "job-profiles-refresh-subscription": "job-profile-segment-refresh",
    "job-profiles-refresh-topic": "job-profile-refresh",
    "service-bus-connection-string": "[service bus connection string]"
  },
  "jobProfileClientOptions": {
    "Timeout": "00:00:10",
    "BaseAddress": "https://localhost"
  },
  "Policies": {
    "HttpCircuitBreaker": {
      "DurationOfBreak": "00:01:00",
      "ExceptionsAllowedBeforeBreaking": 3
    },
    "HttpRetry": {
      "BackoffPower": 2,
      "Count": 3
    }
  },
  "RefreshPolicies": {
    "HttpCircuitBreaker": {
      "DurationOfBreak": "00:00:02",
      "ExceptionsAllowedBeforeBreaking": 3
    },
    "HttpRetry": {
      "BackoffPower": 2,
      "Count": 3
    }
  }
}